{
  "name": "goto-folder",
  "displayName": "Quick Open Folder",
  "description": "Hop over to any folder you want by simply typing its name.",
  "version": "0.0.3",
  "publisher": "andrei-ignatev",
  "license": "MIT",
  "icon": "resources/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-ignatev/goto-folder.git"
  },
  "keywords": [
    "navigation",
    "goto",
    "quick open"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "goto-folder.find-folder",
        "title": "Go to Folder..."
      }
    ],
    "keybindings": [
      {
        "command": "goto-folder.find-folder",
        "key": "shift+ctrl+alt+m",
        "mac": "shift+cmd+opt+m"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "debounce": "^1.2.1"
  }
}